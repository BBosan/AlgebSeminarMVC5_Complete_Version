@model IEnumerable<Se.Models.SemVM>

@using Se.Models;
@using Se.Controllers;

@{ 
    Layout = null;
}


<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <script src="//cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/jqueryui-touch-punch/0.2.3/jquery.ui.touch-punch.min.js"></script>
    <style>
        #feedback {
        font-size: 1.2em;
        }

        #selectable .ui-selecting {
        background: #FECA40;
        }

        #selectable .ui-selected {
        background: #F39814;
        color: white;
        border: 1px solid #191919;
        }

        #selectable {
        list-style-type: none;
        margin: 0;
        padding: 0;
        width: 60%;
        }

        #other_table_container table {
        /*border: 1px solid black;*/
        table-layout: fixed;
        width: 100%;
        /*margin:0 auto;*/
        /*background:#eee;*/
        }

        #other_table_container table td {
        /*border: 1px solid black;*/
        table-layout: fixed;
        overflow: hidden;
        /*height: 40px;*/
        }

        #other_table_container table th {
        /*border: 1px solid black;*/
        overflow: hidden;
        /*height: 30px;*/
        vertical-align: middle;
        }

        #za_brisat_submit2, #clear2 {
        padding: 0 !important;
        margin: 0 !important;
        width: 100%;
        height: 100%;
        opacity: 0;
        overflow:hidden;
        white-space: nowrap;
        }


        #za_brisat_submit2 span, #clear2 span {
        color: #eee;
        }

        .container_for_button {
        padding: 3%;
        width: 100%;
        height: 100%;
        }

        .test {
        color: red;
        width:0;
        padding: 0 !important; /*padding stvara stekanje pri animaciji*/
        }

        .hidden_row #naslov {
        overflow: hidden;
        white-space: nowrap;
        text-decoration: underline;
        color: dodgerblue;
        /*display:inline-block;*/
        /*border-bottom:1px dashed red;*/
        /*line-height: 0.85;*/
        }

        .hidden_row .test{
         height: 30px;
        }

    </style>
</head>

<br>

<div class="container">
    <div id="Ajax">
        @{Html.RenderPartial("_MultiSelectTablePartial2", Model);}
    </div>
    <p id="feedback">
        <span>You've selected:</span> <span id="select-result">none</span>
    </p>
</div>





